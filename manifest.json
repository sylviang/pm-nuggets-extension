{
  "manifest_version": 3,
  "name": "Nuggets of Wisdom",
  "description": "Become a better product manager by learning one bite-sized concept, one tab at a time.",
  "version": "1.0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "newtab.html"
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self';"
  }
}